{
  "name": "npm-next",
  "version": "33.0.1",
  "description": "wraps the latest version of npm for testing",
  "main": "index.js",
  "bin": {
    "npm-next": "bin/index.js"
  },
  "scripts": {
    "test": "npm run lint && tape test.js",
    "lint": "standard",
    "start": "npm install --cache-min=0 npm@next && npm test"
  },
  "author": "Tim Oxley <secoif@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "npm": "next"
  },
  "devDependencies": {
    "standard": "^8.6.0",
    "tape": "~2.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/timoxley/npm-next.git"
  },
  "bugs": {
    "url": "https://github.com/timoxley/npm-next/issues"
  },
  "homepage": "https://github.com/timoxley/npm-next",
  "keywords": [
    "npm",
    "beta",
    "alpha",
    "version",
    "semver",
    "testing",
    "test"
  ]
}
